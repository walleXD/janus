{
  "name": "janus",
  "version": "0.1.0",
  "description": "A very simple CLI implementation of game of life",
  "bin": {
    "janus": "bin/index.js"
  },
  "main": "dist/index.js",
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "dev:run": "nodemon --watch src --ext ts,tsx --exec node --require ts-node/register src/index.ts",
    "dev": "concurrently npm:typecheck:watch npm:dev:run --raw",
    "debug": "nodemon --watch src --ext ts,tsx --exec node --inspect-brk=0.0.0.0:9229 --require ts-node/register src/index.ts",
    "prebuild": "rm -rf dist/",
    "build": "tsc -b",
    "start": "node --require tsconfig-paths/register dist/index.js",
    "lint": "eslint . --ext .js,.ts,.tsx --quiet",
    "lint:fix": "npm run lint:js -- --fix",
    "check:pkgs": "npx syncpack",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "npm run typecheck -- --watch",
    "hook:add": "husky add",
    "hook:prepare": "husky install",
    "hook:pre-commit": "lint-staged",
    "hook:commit-msg": "commitlint --edit $1"
  },
  "keywords": [
    "game",
    "life",
    "cli",
    "cli-game-of-life"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@commitlint/config-lerna-scopes": "15.0.0",
    "@swc/core": "1.2.121",
    "@swc/helpers": "0.3.2",
    "@swc/jest": "0.2.15",
    "@types/ink-big-text": "1.2.1",
    "@types/ink-box": "1.0.2",
    "@types/ink-gradient": "2.0.1",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.14",
    "@types/react": "17.0.37",
    "@typescript-eslint/eslint-plugin": "5.6.0",
    "commitlint": "15.0.0",
    "concurrently": "6.5.1",
    "dotenv": "10.0.0",
    "eslint": "8.4.1",
    "eslint-config-standard-with-typescript": "21.0.1",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "5.2.0",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "husky": "7.0.4",
    "jest": "27.4.5",
    "lerna": "4.0.0",
    "lint-staged": "12.1.3",
    "nodemon": "2.0.15",
    "prettier": "2.5.1",
    "prettier-config-standard": "4.0.0",
    "regenerator-runtime": "0.13.9",
    "ts-node": "10.4.0",
    "tsconfig-paths": "3.12.0",
    "tslib": "2.3.1",
    "typescript": "4.5.4",
    "zx": "4.2.0"
  },
  "dependencies": {
    "hashids": "2.2.10",
    "ink": "3.2.0",
    "ink-big-text": "1.2.0",
    "ink-box": "1.0.0",
    "ink-color-pipe": "3.0.0",
    "ink-gradient": "2.0.0",
    "ink-text-input": "4.0.2",
    "react": "17.0.2"
  }
}
