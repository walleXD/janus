  "scripts": {
    "dev:run": "nodemon --watch src --ext ts,tsx --exec node --require ts-node/register --require tsconfig-paths/register --require dotenv/config src/index.ts",
    "dev": "concurrently npm:typecheck:watch npm:dev:run --raw",
    "debug": "nodemon --watch src --ext ts,tsx --exec node --inspect-brk=0.0.0.0:9229 --require ts-node/register --require tsconfig-paths/register --require dotenv/config src/index.ts",
    "lint": "eslint . --ext .js,.ts,.tsx,.ate --quiet",
    "lint:fix": "npm run lint:js -- --fix",
    "check:pkgs": "npx syncpack",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "npm run typecheck -- --watch",
    "hook:add": "husky add",
    "hook:prepare": "husky install",
    "hook:pre-commit": "lint-staged",
    "hook:commit-msg": "commitlint --edit $1"
  },
  "keywords": [],
  "author": "",
