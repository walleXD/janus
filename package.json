    "@types/jest": "27.0.3",
    "@types/node": "16.11.12",
    "@types/react": "^17.0.37",
    "@typescript-eslint/eslint-plugin": "5.6.0",
    "commitlint": "15.0.0",
    "concurrently": "6.4.0",
